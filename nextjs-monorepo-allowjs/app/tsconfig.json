{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "config": [
        "./config"
      ],
      "components": [
        "./components"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "incremental": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "./**/*.ts",
    "./**/*.js",
    "./**/*.jsx",
    "./**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
