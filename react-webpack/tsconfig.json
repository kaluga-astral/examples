{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "target": "es5",
    "strict": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "common/*": [
        "./common/*"
      ],
    },
    },
  "include": ["./ts-modules/**/*.ts", "./src/**/*.ts", "./src/**/*.tsx", "./global.d.ts"]
}
